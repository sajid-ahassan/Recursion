#include <bits/stdc++.h>
using namespace std;

#define all(x) (x).begin(), (x).end()
#define allr(x) (x).rbegin(), (x).rend()
#define gsize(x) (int)((x).size())
// #define int long long
// #define endl '\n'
bool comparePairs(const pair<int, int> &a, const pair<int, int> &b)
{
    if (a.first != b.first)
        return a.first > b.first;
    return a.second < b.second;
}
#define optimize()                \
    ios_base::sync_with_stdio(0); \
    cin.tie(0);                   \
    cout.tie(0);
// const int mod = 1e10 + 7;

void pri(int n, int ct, vector<int> arr, vector<int> v)
{
    if (ct >= n)
    {
        for (auto it : v)
            cout << it << ' ';
        cout << endl;
        return;
    }
    v.push_back(arr[ct]);
    pri(n, ct + 1, arr, v);
    v.pop_back();
    pri(n, ct + 1, arr, v);
}
signed main()
{
    optimize();
    int n;
    cin >> n;
    vector<int> arr(n);
    vector<int> v;
    for (auto &it : arr)
        cin >> it;
    pri(n, 0, arr, v);

    return 0;
}
